  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "dev": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "typecheck": "tsc --noEmit",
    "lint": "run-s lint:eslint lint:stylelint:css lint:prettier",
    "lint:eslint": "eslint --ext .js,.jsx,.ts,.tsx,.vue ./src",
    "lint:stylelint:css": "stylelint ./src/**/*.{css,scss,sass,vue}",
    "lint:prettier": "prettier --check ./src/**/*.{js,jsx,ts,tsx,vue,css,scss,sass}",
    "fix": "run-s fix:eslint fix:prettier fix:stylelint:css",
    "fix:eslint": "eslint --fix --ext .js,.jsx,.ts,.tsx,.vue ./src",
    "fix:stylelint:css": "stylelint --fix ./src/**/*.{css,scss,sass,vue}",
    "fix:prettier": "prettier --write ./src/**/*.{js,jsx,ts,tsx,vue,css,scss,sass}"
  },
  "dependencies": {
    "@mdx-js/react": "^2.1.5",
    "gatsby": "^4.24.7",
